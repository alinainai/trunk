<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginBottom="10dp"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/dimen_32"
        android:layout_height="wrap_content" >

        <ImageView
            android:layout_weight="1"
            android:id="@+id/home_btn_sever"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_25"
            android:src="@mipmap/hone_btn_sever" />
        <ImageView
            android:layout_weight="1"
            android:id="@+id/home_btn_activ"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_80"
            android:src="@mipmap/home_btn_activ" />
        <ImageView
            android:layout_weight="1"
            android:id="@+id/home_btn_ency"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_80"
            android:src="@mipmap/home_btn_ency" />
        <ImageView
            android:layout_weight="1"
            android:id="@+id/home_btn_phy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dimen_80"
            android:layout_marginEnd="@dimen/dimen_25"
            android:src="@mipmap/home_btn_phy" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/dimen_30"
        android:layout_height="wrap_content" >

        <ImageView
            android:layout_weight="1"
            android:id="@+id/home_btn_bas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_25"
            android:src="@mipmap/home_btn_bas" />
        <ImageView
            android:layout_weight="1"
            android:id="@+id/home_btn_quan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_80"
            android:src="@mipmap/home_btn_quan" />
        <ImageView
            android:layout_weight="1"
            android:id="@+id/home_btn_help"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_80"
            android:src="@mipmap/home_btn_help" />
        <ImageView
            android:layout_weight="1"
            android:id="@+id/home_btn_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dimen_80"
            android:layout_marginEnd="@dimen/dimen_25"
            android:src="@mipmap/home_btn_all" />

    </LinearLayout>


    <TextView
        android:background="@drawable/layout_selector_bg"
        android:padding="30dp"
        android:gravity="center"
        android:text="11111111"
        android:id="@+id/tv_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />


</LinearLayout>