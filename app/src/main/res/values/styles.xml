    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#8fc450</item>
        <item name="colorPrimaryDark">#8fc450</item>
        <item name="colorAccent">#eee</item>
        <item name="colorButtonNormal">#8fc450</item>
        <item name="colorControlHighlight">#eee</item>
        <item name="alertDialogTheme">@style/Theme.AppCompat.Light.Dialog.Alert.Self</item>
    </style>

    <!--设置dialogbutton颜色-->
    <style name="Theme.AppCompat.Light.Dialog.Alert.Self" parent="@style/Theme.AppCompat.Light.Dialog.Alert">
        <!--修改AlertDialog按钮的颜色-->
        <item name="colorAccent">#66aa01</item>
    </style>

    <style name="AppThemeNoStatus" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="divider">
        <item name="android:background">@color/base_divider_color</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/px_1</item>
    </style>

    <style name="tv_color_33_size_26">
        <item name="android:textSize">@dimen/font_size_26</item>
        <item name="android:textColor">@color/base_text_color</item>
    </style>

    <style name="MyRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_bar_full</item>
        <item name="android:minHeight">@dimen/px_24</item>
        <!-- - - 评分条最小高度和最大高度调整 -->
        <item name="android:maxHeight">@dimen/px_26</item>
    </style>

    <!-- 标题栏 -->

    <style name="tv_color_66_size_32">
        <item name="android:textSize">@dimen/font_size_32</item>
        <item name="android:textColor">@color/base_text_info_color</item>
    </style>

    <style name="tv_color_white_size_38">
        <item name="android:textSize">@dimen/font_size_38</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="tv_color_black_size_34">
        <item name="android:textSize">@dimen/font_size_34</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <!-- 底部弹出框 -->
    <style name="transparentFrameWindowStyle" parent="android:style/Theme.Dialog">
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="tv_color_black_size_26">
        <item name="android:textSize">@dimen/font_size_26</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="tv_color_33_size_28">
        <item name="android:textSize">@dimen/font_size_28</item>
        <item name="android:textColor">@color/base_text_color</item>
    </style>

    <style name="tv_color_white_size_28">
        <item name="android:textSize">@dimen/font_size_28</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="tv_color_red_size_28">
        <item name="android:textSize">@dimen/font_size_28</item>
        <item name="android:textColor">@color/red</item>
    </style>

    <style name="tv_color_cc_size_24">
        <item name="android:textSize">@dimen/font_size_24</item>
        <item name="android:textColor">@color/base_search_hint_color</item>
    </style>


    <style name="custom_dialog" parent="@android:style/Theme.Dialog">
        <!-- 边框 -->
        <item name="android:windowFrame">@null</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否透明 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 背景透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">false</item>

    </style>


    <style name="progress_dialog_info" parent="@style/custom_dialog">
        <!-- 模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:background">@android:color/transparent</item>
    </style>


    <!-- 底部弹出框的动画-->
    <style name="main_menu_anim_style">
        <item name="android:windowEnterAnimation">@anim/photo_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/photo_dialog_out_anim</item>
    </style>

    <!-- 底部弹出框的动画-->
    <style name="zoom_anim_style">
        <item name="android:windowEnterAnimation">@anim/zoomin</item>
        <item name="android:windowExitAnimation">@anim/zoomout</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Theme.UMDefault" parent="android:style/Theme.NoTitleBar"></style>

    <style name="DialogBottom">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="@android:windowExitAnimation">@anim/dialog_exit</item>
    </style>
        <!--未完成 -->
    <!-- 首页fragment的style-->

    <style name="home_ll_icon">
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="home_button_icon">
        <item name="android:scaleType">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="home_text_icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/px_14</item>
        <item name="android:textColor">#666</item>
        <item name="android:textSize">@dimen/px_24</item>
    </style>

    <style name="home_text_a">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/px_24</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="home_text_b">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#666</item>
        <item name="android:textSize">@dimen/px_20</item>
    </style>

    <style name="home_img_a">
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_width">@dimen/px_80</item>
        <item name="android:layout_height">@dimen/px_80</item>
        <item name="android:layout_marginTop">@dimen/px_8</item>
    </style>

    <style name="home_img_b">
        <item name="android:layout_marginLeft">@dimen/px_20</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_width">@dimen/px_136</item>
        <item name="android:layout_height">@dimen/px_136</item>
    </style>

    <style name="home_super_con">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/px_20</item>
        <item name="android:textColor">@color/cff5000</item>
    </style>
    <style name="home_super_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">#333</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/px_24</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="home_super_img">
         <item name="android:scaleType">fitCenter</item>
         <item name="android:layout_width">@dimen/px_120</item>
         <item name="android:layout_height">@dimen/px_120</item>
         <item name="android:layout_marginTop">@dimen/px_8</item>
    </style>

</resources>
